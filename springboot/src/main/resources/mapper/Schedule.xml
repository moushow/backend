<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.mapper.ScheduleMapper">
    <update id ="update">
        update schedule
        <set>
            <if test="date != null">
                date = #{date},
            </if>
            <if test="state != null">
                state = #{state},
            </if>
            <if test="event != null">
                event = #{event},
            </if>
            <if test="remark != null">
                remark = #{remark}
            </if>
        </set>
        <where>
            id = #{id}
        </where>
    </update>
</mapper>
